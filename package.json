{
  "name": "@wemake-ai/mcp",
  "version": "25.1.1",
  "author": "WeMake (https://wemake.cx)",
  "devDependencies": {
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.31.0",
    "markdownlint": "^0.38.0",
    "prettier": "^3.6.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "bugs": "https://github.com/WeMake-ai/mcp/issues",
  "description": "WeMake Model Context Protocol servers",
  "homepage": "https://wemake.cx",
  "license": "MIT",
  "prettier": {
    "trailingComma": "none",
    "printWidth": 80,
    "proseWrap": "always"
  },
  "private": true,
  "scripts": {
    "build:sequential-thinking": "bun build --target=node --outfile=src/sequential-thinking/dist/index.js src/sequential-thinking/index.ts",
    "publish:sequential-thinking": "bun publish --cwd=src/sequential-thinking --access public",
    "publish-all": "bun publish --workspaces --access public",
    "link-all": "bun link --workspaces",
    "format": "bun prettier && bun lint --fix",
    "prettier": "bunx prettier --write .",
    "lint": "bunx eslint . --ext .ts,.tsx"
  },
  "type": "module",
  "workspaces": [
    "src/*"
  ]
}
