{
  "name": "@wemake-ai/analogical-reasoning",
  "version": "0.1.3",
  "description": "MCP server for analogical reasoning: structured pattern mapping, domain-to-domain structure alignment, and explainable inference",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "bun": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WeMake-AI/mcp.git",
    "directory": "src/analogical-reasoning"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/WeMake-AI/mcp/tree/main/src/analogical-reasoning",
  "author": "heyFlorentin",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "thinking",
    "model-enhancement"
  ],
  "bin": {
    "mcp-server-analogical-reasoning": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun tsc && shx chmod +x dist/*.js",
    "types": "bun tsc --declaration --emitDeclarationOnly --declarationDir dist/types",
    "prepare": "bun run types && bun run build",
    "publish": "bun publish",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4",
    "chalk": "^5.6.0"
  },
  "devDependencies": {
    "@types/node": "^22.17.2",
    "shx": "^0.3.4",
    "typescript": "^5.9.2"
  }
}
